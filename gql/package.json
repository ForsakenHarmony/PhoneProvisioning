{
  "name": "phone-gql-server",
  "version": "1.3.0",
  "bin": "dist/index.js",
  "main": "src/index.ts",
  "author": "leah@hrmny.sh",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "tsnd --no-notify --respawn .",
    "debug": "tsnd --respawn --inspect .",
    "build": "tsc && pkg --public -t node10-win-x64 -o ../release/PhoneProvisioning_1.3.0 .",
    "build:ts": "tsc",
    "create-migration": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "generate-migration": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "prettier": "prettier src/**/*.{ts,js,json} --write"
  },
  "dependencies": {
    "apollo-server": "^2.25.4",
    "arpping": "^0.2.0",
    "axios": "^0.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "express": "^4.17.1",
    "flru": "^1.0.2",
    "graphql": "^14.5.8",
    "open": "^6.4.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "type-graphql": "^0.17.5",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.19",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "nodemon": "^1.19.3",
    "pkg": "^4.4.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.4"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "dist/public/**/*"
  }
}
