{
  "name": "phone-gql-server",
  "version": "1.1.1",
  "bin": "dist/index.js",
  "main": "src/index.ts",
  "author": "me@hrmny.sh",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "tsnd  --no-notify --respawn .",
    "debug": "tsnd --respawn --inspect .",
